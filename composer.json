{
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4.0",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^1.6.1",
        "slim/slim": "^4.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "zendframework/zend-config-aggregator": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    }
}
